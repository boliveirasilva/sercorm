{
    "name": "boliveirasilva/sercorm",
    "description": "A simple ORM for php5.6 implemented over ADOdb library",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Oliveira",
            "email": "boliveirasilva@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6",
        "ext-json": "*",
        "ext-mbstring": "*",
        "adodb/adodb-php": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "SercORM\\": "database/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SercTest\\": "tests/"
        }
    }
}
